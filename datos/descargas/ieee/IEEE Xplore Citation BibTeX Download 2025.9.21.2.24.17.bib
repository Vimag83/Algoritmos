  ISSN={2768-4644},
  month={Jan},}@INPROCEEDINGS{10567144,
  author={Kong, Jiaze and Shen, Xiaobing and Sabariego, Ruth V. and Martinez, Wilmar},
  booktitle={2024 IEEE 10th International Power Electronics and Motion Control Conference (IPEMC2024-ECCE Asia)}, 
  title={Cross-cut heatsink optimization design by applying Conditional Generative Adversarial Network}, 
  year={2024},
  volume={},
  number={},
  pages={4155-4160},
  abstract={Heatsinks have significantly evolved due to demands for larger, cost-effective, and longer-lasting power electronics. Traditional simulation methods for predicting temperatures are time-consuming, requiring complex computations. Artificial Intelligence (AI), particularly Conditional Generative Adversarial Networks (CGaNet), offers a faster, more accurate alternative. CGaNet demonstrated superior temperature prediction accuracy on various datasets, including those outside its training range. The initial training involved a dataset from a cross-cut heatsink, including parameters like pin dimensions, input power, and fan speed. CGaNet predictions for an external dataset showed high conformity to actual data, with the coeffient of determination (R2) of 0.941, surpassing the Feed-forward Neural Network (FNN) in extrapolation ability.},
  keywords={Training;Temperature distribution;Accuracy;Generative adversarial networks;Power electronics;Pins;Heat sinks;Cross-cut heatsink;Conditional Generative Adversarial Network;Feedforward Neural Network;extrapolation;topology optimization;Artificial Intelligent},
  doi={10.1109/IPEMC-ECCEAsia60879.2024.10567144},
  ISSN={},
  month={May},}@INPROCEEDINGS{11099457,
  author={Jiang, Weiwei and Mu, Jianbin and He, Miao and Gu, Weixi and Cheng, Xirong and Azar, Ahmad Taher and Kamal, Nashwa Ahmad},
  booktitle={2025 International Conference on Control, Automation and Diagnosis (ICCAD)}, 
  title={Bankruptcy Prediction with Imbalanced Data and Conditional Generative Adversarial Networks}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={The corporate bankruptcy prediction problem remains unresolved, primarily due to the significant class imbalance caused by the rarity of bankruptcy cases in most financial markets. This study addresses the class imbalance challenge using generative artificial intelligence (GAI), employing a conditional generative adversarial network (CTGAN) to generate synthetic data for training machine learning models. Following data augmentation, six representative machine learning models, including Decision Tree (DT), Random Forest (RF), AdaBoost, XGBoost, Artificial Neural Network (ANN), and LightGBM, are evaluated and compared using two real-world datasets from Taiwan and the USA. The CTGAN-based synthetic data generation approach is further compared with alternative methods, including the Synthetic Minority Over-sampling Technique (SMOTE) and Gaussian copula data generators. Experimental results demonstrate that the workflow combining CTGAN and LightGBM achieves the highest prediction performance, measured by F1 score, on both datasets. It also outperforms baseline methods from previous studies, highlighting the effectiveness of GAI in addressing the class imbalance problem in corporate bankruptcy prediction.},
  keywords={Training;Generative AI;Training data;Artificial neural networks;Generative adversarial networks;Generators;Data models;Bankruptcy;Random forests;Synthetic data;Artificial Intelligence;Bankruptcy Prediction;Conditional Generative Adversarial Network;Generative AI},
  doi={10.1109/ICCAD64771.2025.11099457},
  ISSN={2767-9896},
  month={July},}@INPROCEEDINGS{10967941,
  author={Yang, Nan and Dong, Liang and Wei, Yichen and Liu, Xingwei and Chen, Weitong and Zhang, Zhejun and Li, Xinhang and Huang, Yupeng and Li, Lei},
  booktitle={2025 IEEE 8th Information Technology and Mechatronics Engineering Conference (ITOEC)}, 
  title={GARL: A Generative Adversarial Reinforcement Learning for Transferable Power Grid Dispatch}, 
  year={2025},
  volume={8},
  number={},
  pages={1224-1228},
  abstract={This paper proposes a novel deep reinforcement learning approach, named GARL (Generative Adversarial Reinforcement Learning), for intra-day optimization dispatch in high-proportion new energy grid. The proposed method embeds a Generative Adversarial Network (GAN) within the Deep Deterministic Policy Gradient (DDPG) framework to learn the reward distribution of the power grid in the future operation situation, so as to realize the cumulative reward prediction in the dispatching period and ensure the optimization of the dispatching decision. Comprehensive experiments are conducted on the SG-126 power grid simulator under three representative scenarios to evaluate the performance and generalization capability of GARL. The results show that GARL can learn the distribution characteristics of stateaction-reward sequence from historical experience, establish the mapping relationship between current state and future cumulative reward, and improve the accuracy of future power grid operation trend prediction, which is always better than the independent DDPG algorithm. Furthermore, the study explores the potential of transfer learning between scenarios and finds that GARL exhibits a promising generalization capability.},
  keywords={Renewable energy sources;Uncertainty;Transfer learning;Generative adversarial networks;Deep reinforcement learning;Market research;Prediction algorithms;Power grids;Dispatching;Optimization;Deep reinforcement learning;Generative Multiple Adversarial Networks;Power grid dispatch;Renewable energy;Transfer learning;Generalization capability},
  doi={10.1109/ITOEC63606.2025.10967941},
  ISSN={2693-289X},
  month={March},}@ARTICLE{10852323,
  author={Aldraiweesh, Ahmed A. and Alturki, Uthman},
  journal={IEEE Access}, 
  title={The Influence of Social Support Theory on AI Acceptance: Examining Educational Support and Perceived Usefulness Using SEM Analysis}, 
  year={2025},
  volume={13},
  number={},
  pages={18366-18385},
  abstract={This study investigates factors influencing the acceptance of artificial intelligence (AI) in education, focusing on attitudes and intentions toward its use. Drawing on the Technology Acceptance Model (TAM) and Social Support Theory (SST), the research examines perceived usefulness, ease of use, and support measures, including educational and emotional support. The study was conducted at King Saud University, where it adopted a quantitative approach, collecting data from 350 participants through a structured survey. A final sample of 308 valid responses was analyzed using Structural Equation Modeling (SEM). The findings revealed significant relationships between key constructs: perceived educational support positively influenced perceived usefulness ( $\beta = 0.160$ , p = 0.000) and attitudes toward AI ( $\beta = 0.140$ , p = 0.010). Perceived usefulness ( $\beta = 0.670$ , p = 0.000) and ease of use ( $\beta = 0.330$ , p = 0.000) were strong predictors of positive attitudes, which, in turn, significantly shaped intentions to use AI ( $\beta = 0.760$ , p = 0.000). Emotional support and cognitive support also had notable but mixed effects on perceived usefulness. However, social interaction support and perceived enjoyment did not demonstrate significant influence. These findings highlight the importance of designing AI systems that emphasise usability and provide robust educational and emotional support to enhance acceptance. The study contributes to theory by extending TAM with constructs from SST, offering new insights into AI adoption in academic contexts. Practically, the results guide policymakers and developers in fostering AI acceptance by addressing educational and emotional needs, ensuring effective implementation in higher education.},
  keywords={Artificial intelligence;Education;Generative AI;Chatbots;Solid modeling;Technology acceptance model;Stakeholders;Security;Psychology;Numerical analysis;Artificial intelligence;social support theory;AI adoption frameworks;TAM;educational AI},
  doi={10.1109/ACCESS.2025.3534099},
