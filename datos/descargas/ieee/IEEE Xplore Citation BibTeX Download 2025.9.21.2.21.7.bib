  month={June},}@INPROCEEDINGS{10847575,
  author={Kumar, Prem},
  booktitle={2024 IEEE 16th International Conference on Computational Intelligence and Communication Networks (CICN)}, 
  title={Sculpting with Algorithms: A Computational Approach to Analyzing Digital and Traditional Methods}, 
