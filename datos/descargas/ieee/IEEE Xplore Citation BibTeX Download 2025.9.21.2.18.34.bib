  title={Adapting Artificial Intelligence Towards Managing Sustainable Construction}, 
  year={2025},
  volume={},
  number={},
  pages={43-48},
  abstract={This paper provides an overview of how Artificial Intelligence (AI) technologies contribute to sustainable construction, focusing on their applications in design and planning, construction management, material selection, waste management, energy management, and building performance monitoring. AI-driven sustainability metrics and optimization strategies are explored to improve the efficiency and cost-effectiveness of construction processes. Specialized tools such as Generative AI (GenAI), Digital Twins (DT), and Support Vector Machines (SVM) are discussed, highlighting their role in achieving sustainability goals within the built environment. Despite the potential benefits, challenges remain, including technological, fi-nancial, regulatory, and workforce skillset gaps. Through industry examples and case studies, the paper demonstrates AI's impact on energy efficiency, water conservation, waste reduction, and carbon footprint reduction. The paper highlights the promising future of AI in sustainable construction and recommends that future research focus on overcoming integration challenges, enhancing workforce training, and further exploring AI's role in advancing circular economy practices within the construction industry.},
  keywords={Waste reduction;Support vector machines;Measurement;Water conservation;Energy efficiency;Artificial intelligence;Sustainable development;Optimization;Monitoring;Carbon footprint;Sustainable Construction;Construction Management;Artificial Intelligence;Machine Learning;Deep Learning;Sustainability},
  doi={10.1109/WiDS-PSU64963.2025.00022},
  ISSN={},
  month={April},}@INPROCEEDINGS{11014955,
  author={Salman, Amany},
